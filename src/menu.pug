- var navmenu = { Menu: '/', Nosotros: '/about' }

nav.navbar.navbar-dark.navbar-inverse(role='navigation')
  div.navbar-header
    button(type='button',data-toggle='collapse',data-target='#jcs-navbar-collapse-1').navbar-toggle
      span.sr-only Toggle navigation
      span.iconbar
      span.iconbar
      span.iconbar
    a(href='#').navbar-brand ManitoManitas
  div#jcs-navbar-collapse-1.collapse.navbar-collapse
    ul.nav.navbar-nav

      each href, key in navmenu
        if key == pagekey
          li.active: a(href=href)= key
        else
          li: a(href=href)= key

    
    form(role='search').navbar-form.navbar-right
      div.form-group
        input(type='text',placeholder='Search').form-control
      | &nbsp;
      button(type='submit').btn.btn-default Submit
